<merge xmlns:carloz="http://schemas.android.com/apk/res/com.carloz.inputmethod" >

    <Row>
        <Key
            carloz:keySpec="1"
            carloz:keyStyle="numKeyStyle" />
        <Key
            carloz:keySpec="2"
            carloz:keyStyle="numKeyStyle" />
        <Key
            carloz:keySpec="3"
            carloz:keyStyle="numKeyStyle" />
        <Key
            carloz:keyStyle="deleteKeyStyle"
            carloz:keyWidth="fillRight" />
    </Row>
    <Row>
        <Key
            carloz:keySpec="4"
            carloz:keyStyle="numKeyStyle" />
        <Key
            carloz:keySpec="5"
            carloz:keyStyle="numKeyStyle" />
        <Key
            carloz:keySpec="6"
            carloz:keyStyle="numKeyStyle" />
        <Key carloz:keyStyle="numSpaceKeyStyle" />
    </Row>
    <Row>
        <Key
            carloz:keySpec="7"
            carloz:keyStyle="numKeyStyle" />
        <Key
            carloz:keySpec="8"
            carloz:keyStyle="numKeyStyle" />
        <Key
            carloz:keySpec="9"
            carloz:keyStyle="numKeyStyle" />
    </Row>
    <Row>
        <Key
            carloz:keySpec=","
            carloz:keyStyle="numKeyStyle" />
        <Key
            carloz:keySpec="0"
            carloz:keyStyle="numKeyStyle" />
        <Key
            carloz:keySpec="."
            carloz:keyStyle="numKeyStyle" />
        <Key
            carloz:keyStyle="enterKeyStyle"
            carloz:keyWidth="fillRight" />
    </Row>

</merge>